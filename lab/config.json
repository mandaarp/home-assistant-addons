{
    "name": "Home Asisstant Lab",
    "version": "latest",
    "slug": "home_assistant_lab",
    "description": "Home Assistant Lab Add-on by Mandar Patil",
    "arch": ["amd64", "i386"],
    "url": "https://github.com/mandaarp/home-assistant-addons",
    "startup": "application",
    "boot": "auto",
    "options": {
        "ssl": true,
        "certfile": "fullchain.pem",
        "keyfile": "privkey.pem",
        "require_ssl": true,
        "system_packages": [],
        "npm_packages": [],
        "init_commands": []
    },
    "schema": {},
    "webui": "[PROTO:ssl]://[HOST]:[PORT:8888]",
    "ports": {
        "8888/tcp": 18888
    },
    "ports_description": {
        "8888/tcp": "Notebook port"
    },
    "host_network": false,
    "host_ipc": false,
    "host_dbus": false,
    "host_pid": false,
    "devices": [],
    "udev": false,
    "auto_uart": true,
    "homeassistant": "",
    "hassio_role": "manager",
    "hassio_api": true,
    "homeassistant_api": true,
    "docker_api": false,
    "privileged": [],
    "full_access": false,
    "apparmor": false,
    "map": [
    "config:rw",
    "share:rw",
    "ssl"
    ],
    "environment" : {},
    "audio": false,
    "gpio": true,
    "devicetree": false,
    "kernel_modules": false,
    "stdin": false,
    "legacy": false,
    "image": "mandaarp/home-assistant-lab",
    "timeout": 10,
    "discovery": [],
    "services": [],
    "auth_api": true,
    "ingress": true,
    "ingress_port": 8888,
    "ingress_entry": "",
    "panel_icon": "mdi:puzzle",
    "panel_title": "Home Assistant Lab",
    "panel_admin": true
  }
